{
  "name": "starter",
  "version": "1.0.0",
  "description": "starter",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "serve": "run-p watch:* server",
    "server": "node browser-sync.js",
    "ejs": "npx @11ty/eleventy --input=src/ejs --output=dist",
    "sass": "sass src/assets/sass/:dist/assets/css/ --no-source-map -s compressed",
    "postcss": "postcss dist/assets/css/**/*.css --dir dist/assets/css --no-map",
    "ts": "node esbuild.js",
    "build": "run-p build:*",
    "build:ejs": "npm run ejs",
    "build:sass": "npm run sass",
    "build:postcss": "npm run postcss",
    "build:ts": "npm run ts",
    "watch:ejs": "npm run ejs -- --watch --quiet",
    "watch:sass": "npm run sass -- --watch",
    "watch:postcss": "npm run postcss -- -w",
    "watch:ts": "npm run ts -- --watch",
    "lint": "run-p lint:*",
    "lint:stylelint": "stylelint src/assets/sass/",
    "lint:ts": "eslint src/assets/**/*.ts",
    "update": "run-s update:*",
    "update:execute": "ncu",
    "update:package": "ncu -u",
    "update:ci": "npm install --save-dev",
    "prepare": "husky",
    "start": "run-s start:*",
    "start:setup": "npm i connect-ssi browser-sync",
    "start:execution": "npm run server"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.3",
    "connect-ssi": "^1.1.1",
    "cssnano": "^5.1.15",
    "ejs": "^3.1.9",
    "esbuild": "^0.17.13",
    "eslint": "^8.36.0",
    "glob": "^9.3.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-normalize": "^10.0.1",
    "postcss-normalize-charset": "^5.1.0",
    "postcss-scss": "^4.0.6",
    "sass": "^1.49.9",
    "stylelint": "^15.3.0",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-scss": "^4.5.0",
    "typescript": "^5.0.2"
  },
  "volta": {
    "node": "20.10.0"
  }
}
